#c
printf("msg");
puts(a); //print single line of string 
putchar(a); //print single character

#c++
cout<<"msg";

#java8
System.out.print("msg");

#python3
print("msg\n",end = " ")

#javascript
document.getElementById("id").innerHTML = "msg"; //displaying by changing the value of html tag
document.write("msg"); //displaying msg where the code is placed
window.alert("msg"); // show msg on top of browser window
console.log("msg"); //print msg in the browser console


# c#
Console.WriteLine("msg"); // goes to new line after printing
Console.Write("msg"); //doesn't go to new line after printing 
//variable printing just like python 
Console.write("{0} + {1} = {2}",first_num, second_num, result); //formatted printing
